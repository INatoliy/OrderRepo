{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "BookingService"
    }
  },
  "ConnectionStrings": {
    "BookingsContext": "Host=localhost;Port=5433;Database=booking_service_bookings;Username=bookings_admin;Password=admin_bookings"
  }
}